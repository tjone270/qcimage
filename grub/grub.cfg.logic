# QCImage Logic
insmod btrfs
search --no-floppy --fs-uuid --set=root x_LINUX_UUID
insmod gfxterm
insmod tga
set gfxmode=640x480x32
set gfxpayload=keep
terminal_output gfxterm
use_bg=true
background_image /qcimage/resources/qcsre32.tga
search -n -u -s windows x_WINDOWS_UUID
search -n -u -s linux x_LINUX_UUID


default="windows"

set timeout=5
